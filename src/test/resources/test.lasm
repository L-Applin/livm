;; loop example

    push 10
    push 0
.loop:
    dup
    print
    dup 1         ;; duplicate the head of the stack
    dup 1         ;; put the element at index 2 (3rd from the head) back at the head
    eq
    jnz 4         ;; realtive addr, 4 forward
    push 1
    add
    jmp .loop     ;; jump to label
    push 69
    print